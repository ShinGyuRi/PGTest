import com.moyusoft.util.Definitions;

public class InnisfreeGothicBoldEditText extends EditText {

    public InnisfreeGothicBoldEditText(Context context, AttributeSet attrs) {
        super(context, attrs);
        if(Definitions.InnisfreeGothicBold !=null)
            setTypeface(Definitions.InnisfreeGothicBold);
        //setLineSpacing(TypedValue.applyDimension(TypedValue.COMPLEX_UNIT_DIP, 5.0f, getResources().getDisplayMetrics()), 1.0f);
    }

    public InnisfreeGothicBoldEditText(Context context) {
        super(context);
        if (Definitions.InnisfreeGothicBold != null)
            setTypeface(Definitions.InnisfreeGothicBold);
        //setLineSpacing(TypedValue.applyDimension(TypedValue.COMPLEX_UNIT_DIP, 5.0f, getResources().getDisplayMetrics()), 1.0f);
    }

    public InnisfreeGothicBoldEditText(Context context, AttributeSet attrs, int defStyle) {
        super(context, attrs, defStyle);
        if (Definitions.InnisfreeGothicBold != null)
            setTypeface(Definitions.InnisfreeGothicBold);
        //setLineSpacing(TypedValue.applyDimension(TypedValue.COMPLEX_UNIT_DIP, 5.0f, getResources().getDisplayMetrics()), 1.0f);
    }

